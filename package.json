  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/accept-language-parser": "^1.5.7",
    "@types/bun": "^1.2.10",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {},
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "class-transformer": "^0.5.1",
